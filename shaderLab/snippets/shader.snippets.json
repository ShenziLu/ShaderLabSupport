{
    "for":{
        "prefix":"for",
        "body":[
            "for(int ${0:i} = 0; ${0:i} < length; ${0:i}++)",
            "{",
            "\t",
            "}"
        ]
    },
    "if":{
        "prefix":"if",
        "body":[
            "if(${0:true})",
            "{",
            "\t",
            "}"
        ]
    },
    "#if":{
        "prefix":"#if",
        "body":[
            "#if $0",
            "\t",
            "#else",
            "\t",
            "#endif"
        ]
    },
    "#elif":{
        "prefix":"#elif",
        "body":[
            "#if $0",
            "\t",
            "#elif ",
            "\t",
            "#else",
            "\t",
            "#endif"
        ]
    },
    "#ifdef":{
        "prefix":"#ifdef",
        "body":[
            "#ifdef $0",
            "\t",
            "#else",
            "\t",
            "#endif"
        ]
    },
    "#ifndef":{
        "prefix":"#ifndef",
        "body":[
            "#ifndef ${0:${TM_FILENAME/(.*)\\.[a-z]+/${1:/upcase}/i}}",
            "\t#define ${0:${TM_FILENAME/(.*)\\.[a-z]+/${1:/upcase}/i}}",
            "#endif"
        ]
    },
    "CGINCLUDE":{
        "prefix":"CGINCLUDE",
        "body":[
            "CGINCLUDE",
            "$0",
            "ENDCG"
        ]
    },
    "CGPROGRAM":{
        "prefix":"CGPROGRAM",
        "body":[
            "CGPROGRAM",
            "$0",
            "ENDCG"
        ]
    },
    "HLSLINCLUDE":{
