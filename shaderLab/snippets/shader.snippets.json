            "$0",
            "ENDHLSL"
        ]
    },
    "cbuffer":{
        "prefix":"cbuffer_",
        "body":[
            "CBUFFER_START(${0:UnityPerDraw})",
            "\t",
            "CBUFFER_END"
        ]
    },
    "struct":{
        "prefix":"struct",
        "body":[
            "struct $0",
            "{",
            "\t",
            "};"
        ]
    },
    "Shader":{
        "prefix":"Shader",
        "body":[
            "Shader \"${0:CustomShader}\/${TM_FILENAME/(.*)\\.[a-z]+/${1:/ignore}/i}\"",
